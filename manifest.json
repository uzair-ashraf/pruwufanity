{
  "name": "Pruwufanity",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Removes profanity from web pages and changes them to variations of uwu",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "webNavigation"
  ]
}
